type Point =
    x: int
    y: int

def Point.op_add(self, other: Point) Point =
    Point{
        self.x + other.x
        self.y + other.y
    }

def Point.toString(self) string =
    "Point({self.x}, {self.y})"

let pointA = Point{ 10, 20 }
let pointB = Point{ 30, -15 }
log (pointA + <-pointB).toString!