type User =
    username: string

type Server =
    client: User
    region: string
    id: string
    members: []User

type Client =
    login: |string| do none

type Message =
    content: string
    reply: |string| do none
    author: User
    server: Server

type Options =
    onMessage: |Message| do none
    onReady: || do none

ext node client: |Options{}| -> Client = "_discord_client"